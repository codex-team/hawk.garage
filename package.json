{
  "name": "hawk.so",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint \"src/**/*{.css,.vue}\" --fix",
    "lint:js": "eslint src/ --ext .js --ext .ts --ext .vue --fix",
    "serve:prod": "http-server ./dist --push-state -c-0",
    "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s ./public",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "get-schema": "get-graphql-schema https://api.beta.hawk.so/graphql > schema.graphql",
    "get-schema:stage": "get-graphql-schema https://api.stage.hawk.so/graphql > schema.graphql",
    "get-schema:local": "get-graphql-schema http://localhost:4000/graphql > schema.graphql"
  },
  "dependencies": {
    "@hawk.so/javascript": "^2.1.0",
    "@hawk.so/webpack-plugin": "^0.0.10",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "axios": "^0.19.0",
    "codex-notifier": "^1.1.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "highlight.js": "^10.0.2",
    "http-server": "^0.12.0",
    "lodash.mergewith": "^4.6.2",
    "normalize.css": "^8.0.1",
    "postcss-apply": "^0.12.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-simple-vars": "^5.0.2",
    "register-service-worker": "^1.6.2",
    "short-number": "^1.0.7",
    "spa-http-server": "^0.9.0",
    "svgo": "^1.1.0",
    "svgo-loader": "^2.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.0.2",
    "vue-cli-plugin-storybook": "^1.2.1",
    "vue-cli-plugin-svg-sprite": "^1.0.0",
    "vue-cookies": "^1.6.1",
    "vue-i18n": "^8.15.3",
    "vue-infinite-scroll": "^2.0.2",
    "vue-json-viewer": "^2.2.8",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-notes": "5.3.18",
    "@storybook/vue": "5.3.18",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.0.0",
    "eslint-config-codex": "1.3.6",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^6.1.2",
    "get-graphql-schema": "^2.1.2",
    "lint-staged": "^10.2.2",
    "stylelint": "^13.3.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "typescript": "^3.7.4"
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
